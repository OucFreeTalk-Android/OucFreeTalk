  </servlet>  
  <servlet-mapping> 
    <servlet-name>getPost</servlet-name>  
    <url-pattern>/getPost</url-pattern> 
  </servlet-mapping>  
  
  <servlet> 
    <servlet-name>getComment</servlet-name>  
